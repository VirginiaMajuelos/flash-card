---
const response = await fetch("https://jsonplaceholder.typicode.com/posts");
const posts = await response.json();
---

<section class="m-8">
  <h1 class="font-bold">Pubicaciones</h1>

  {
    posts.map((post) => (
      <div>
        <h2>-{post.title}</h2>
        <p>{post.body}</p>
      </div>
    ))
  }
</section>
